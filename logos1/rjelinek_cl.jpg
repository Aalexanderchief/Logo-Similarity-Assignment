	display: block;
	width: 35px;
	height: 35px;
	text-align: center;
	line-height: 35px;
	font-size: 25px;
	border-radius: 20px;
	color: inherit;
	background-color: #E1D0AE;

	/*color: #ff0024;*/
	border: 1px solid black;

	-webkit-transition: all 0.3s;
	transition: all 0.3s;
}

#animace-vyroba #vid-wrapper a.poi.active, #vid-wrapper a.poi:hover {
	background-color: #65411F;
	color: #e6ddbe;
