<script>
    document.getElementById('loginModalForm').addEventListener('submit', function(e) {
        e.preventDefault();
        let tfaCodeField = document.getElementById('loginModalTfaCode');
        if(!tfaCodeField.classList.contains('hidden')) {
            e.target.submit();
            return;
        }
        let formData = new FormData();
        formData.append('user', document.getElementById('loginUserName').value);
        fetch('/?cl=ratfa_verify&fnc=check', {
            'method': 'POST',
            'body': formData
        }).then(function(response) {
            if(response.status === 204) {
                document.getElementById('login-modal-tfa-box').classList.remove('d-none');
                tfaCodeField.classList.remove('hidden');
                                tfaCodeField.focus();
                return;
            } else {
                e.target.submit();
            }
        });
    });
