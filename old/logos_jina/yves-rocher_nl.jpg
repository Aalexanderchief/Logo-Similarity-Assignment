    background: url('/medias/BENL-XMAS24-glitter.png?context=bWFzdGVyfGltYWdlc3wyOTY5NTd8aW1hZ2UvcG5nfHN5c19tYXN0ZXIvaW1hZ2VzL2g0OS9oMzUvOTk3OTY3MDk4Njc4Mi9CRU5MX1hNQVMyNF9nbGl0dGVyLnBuZ3xjOGU3ZmE5YjA3YmM5NzQ1MWQ2ZDk4MTRkZGU0Mjg0MDlkNWY5YjliZTZhZTcxMGZhYzZkNWVjZGE4ZGIwOWUw');
    background-size: contain;
    text-align: center;
    margin-top: 60px;
}
.xmas-video-container{        
    max-width: 71.25rem; 
    margin: auto;
}
.xmas-video { 
    position: relative;
    padding-bottom: 56.25%; 
    height: 0; 
    overflow: hidden; 
    max-width: 100%;
    border-radius: 10px;
} 
.xmas-video iframe, .xmas-video object, .xmas-video embed { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }
/*.xmas-glitter button{
    border-radius: 8px;
    background: var(--Color_doree_noel, #E1CB85);
    padding: 16px 76px;
    border: none;
    margin: 20px 0;
    font-size: 1rem;
    font-weight: 700;
    color: #000;
}*/

/* SENTEURS */
.xmas-senteurs {
    display: flex;
    justify-content: center;
    gap: 60px;
}
.xmas-senteur {
    display: flex;
    flex-direction: column;
    width: 260px;
    height: 515px;
    justify-content: center;
    padding: 16px;
    cursor: pointer;
    transition: transform .3s cubic-bezier(0,0,0.5,1);
    background-size: contain;
}
.xmas-senteur:hover {
    transform: scale(1.0161290323);
}
.xmas-senteur.grenade {
    background-image: url('/medias/BENL-XMAS24-decor-1.png?context=bWFzdGVyfGltYWdlc3w0MjI0NjJ8aW1hZ2UvcG5nfHN5c19tYXN0ZXIvaW1hZ2VzL2hlYi9oMjAvOTk3OTY3MDM5Njk1OC9CRU5MX1hNQVMyNF9kZWNvci0xLnBuZ3xlMGJkODQzY2E4NWVkNTNiNmU0MDM1MTZhN2JjNDk0OTlkYWU0MTc1ZWViZTAwM2ExZjBlYmZmNjBmNzgyN2Jl&quality=100');
    color: #fff;
}
.xmas-senteur.vanille {
    background-image: url('/medias/BENL-XMAS24-decor-2.png?context=bWFzdGVyfGltYWdlc3wzOTAyMDV8aW1hZ2UvcG5nfHN5c19tYXN0ZXIvaW1hZ2VzL2hmMi9oMzQvOTk3OTY3MDk1NDAxNC9CRU5MX1hNQVMyNF9kZWNvci0yLnBuZ3w1ODBkYjc1OGYzY2I2MzExZWUwNzlhYWQ1ZDFkMjk2NjFkYTVhYjY5ZjQ1OGVhODEwN2YzZjA0NzUyNzhiMzZk&quality=100');
    margin-top: 40px;
}
.xmas-senteur.karite {
    background-image: url('/medias/BENL-XMAS24-decor-3.png?context=bWFzdGVyfGltYWdlc3w0NTI0OTN8aW1hZ2UvcG5nfHN5c19tYXN0ZXIvaW1hZ2VzL2gyNC9oMmIvOTk3OTY3MDY5MTg3MC9CRU5MX1hNQVMyNF9kZWNvci0zLnBuZ3xkNDhjMmU3MzRlY2FhODc3NTQzMGQxMGM4ZmU0NzYyOGU2Yjg0M2VhN2U2ZTRkZTU5Y2M3ODZkMDA1M2ZkNWNj&quality=100');
}
.xmas-senteur h3 {
    font-size: 1.75rem;
}
.xmas-senteur p {
    font-size: 1rem;
}
.xmas-senteur a{
    text-decoration: underline;
    text-underline-offset: 8px;
    margin-top: 20px;
}
.xmas-senteur a:hover{
    text-decoration: none;
}

/* TABS */
.xmas-tabs{
    position: relative;
    padding: 40px 16px;
    border-radius: 10px;
    border: 1px solid rgba(225, 203, 133, 0.30);
    margin-top: 60px;
}
.xmas-tabs p{
    color: #fff;
    font-size: 1.375rem;
    margin-bottom: 40px;
    font-weight: 700;
}
.xmas-tabs>div{
    display: flex;
    gap: 10px;
    flex-wrap: wrap;
}
.xmas-tabs a {
    padding: 15px 20px;
    border-radius: 100px;
    border: 1px solid #E1CB85;
    color: #E1CB85;
}
.xmas-tabs img {
    position: absolute;
    bottom: 0;
    right: 0;
}

/* PRODUITS */
.xmas-produits{
    margin: 0 !important;
    padding: 30px 0 0 !important;
}
.xmas-wrapper .grproductboxcontainer{
    background-color: unset !important;
}

/* ENCARTS */
.xmas-encarts{
    display: flex;
    gap: 10px;
    margin-bottom: 120px;
}
.xmas-encarts article{
    position: relative;
}
.xmas-encarts article img{
    border-radius: 10px;
}
.xmas-encarts article>div{
    background: #fff;
    padding: 10px;
    position: absolute;
    left: 50%;
    transform: translate(-50%, -50%);
    text-align: center;
    border-radius: 4px;
    width: 290px;
    height: 124px;
}
.xmas-encarts article>div h3{
    font-size: 1rem;
    font-weight: 700;
    margin-bottom: 25px;
}
.xmas-encarts article>div a{
    text-decoration: underline;
    text-underline-offset: 8px;
}
.xmas-encarts article>div a:hover{
    text-decoration: none;
}

/* CATEGORIES */
.xmas-cat{
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 20px 0;
}
.xmas-cat h2{
    margin-right: 70px;
}
.xmas-cat ul{
    display: flex;
    gap: 22px;
    flex-wrap: wrap;
    max-width: 40%;
    list-style-type: none;
    text-align: center;
    color: #000;
    font-size: 1rem;
    padding: 0;
    margin-bottom: 0;
}
.xmas-cat ul img{
    display: block;
    margin-bottom: 13px;
}

/* FAQ */
.xmas-faq{
    color: #fff;
    font-size: 1.375rem;
    margin-top: 40px;
    font-weight: 700;
}
.accordion {
  list-style-type: none;
  padding: 0 0 40px;
}
.accordion .item {
  border-bottom: 1px solid #CCCCCC;
}
.accordion .item button {
  position: relative;
  display: block;
  border: none;
  width: 100%;
  background: none;
  outline: none;
  padding: 1.5rem 0;
}
.accordion .item .icon {
  display: inline-block;
  position: absolute;
  top: 30px;
  right: 0;
  width: 17px;
  height: 7px;
  background: url('/medias/BENL-XMAS24-down-arrow.svg?context=bWFzdGVyfGltYWdlc3w1MjF8aW1hZ2Uvc3ZnK3htbHxzeXNfbWFzdGVyL2ltYWdlcy9oOTUvaDIwLzk5Nzk2NzAzNjQxOTAvQkVOTF9YTUFTMjRfZG93bi1hcnJvdy5zdmd8ODI2Y2UyMzY2ODBiNDViODBlZjAwN2YxN2UxOTgyZGM0MjMzM2VkNjRkOGMxMjdiMWIwYTVkZGIzZDExMjdmMw');
}
button[aria-expanded='true'] .icon {
  background: url('/medias/BENL-XMAS24-up-arrow.svg?context=bWFzdGVyfGltYWdlc3w1MTF8aW1hZ2Uvc3ZnK3htbHxzeXNfbWFzdGVyL2ltYWdlcy9oYmEvaDJhLzk5Nzk2NzA2NTkxMDIvQkVOTF9YTUFTMjRfdXAtYXJyb3cuc3ZnfGFkMjRhNzU2MjA5MTg2YTg5MzM4NmEzMzRkMTdkMGQ4ODVhNmI0NGYxNGVhMDBmODFjOGViYzk2MTMxMWIwMGE') !important;
}
.accordion-content.expanded {
  opacity: 1;
  max-height: 80em;
  transition: all 200ms linear;
  will-change: opacity, max-height;
}
.accordion-content {
  opacity: 0;
  max-height: 0;
  overflow: hidden;
  transition: opacity 200ms linear, max-height 200ms linear;
  will-change: opacity, max-height;
}
.accordion-content p {
  padding-right: 4.375rem;
}

@media (max-width: 60rem) {
    .xmas-wrapper h2:not(.product-name){
        font-size: 1.5rem;
        padding: 50px 16px 0;
    }

    /* FLEURS */
    .xmas-wrapper .xmas-fleur{
        width: 110px;
    }
    .xmas-wrapper .xmas-fleur:first-of-type{
        top: 42.5%;
    }
    .xmas-wrapper .xmas-fleur:last-of-type{
        bottom: 37.5%;
    }

    /* PADDING */
    .xmas-video-container {
        padding: 0 16px;
    }
    .xmas-senteurs{
        padding: 0 0 0 16px;
    }
    /* VIDEO */
    .xmas-glitter{
        background-size: 260%;
    }
    .xmas-video{
        height: calc(100vw - 32px);
    }
    /*.xmas-glitter button {
        padding: 16px 0;
        width: 100%;
    }*/

    /* SENTEURS */
    .xmas-senteurs {
        display: flex;
        justify-content: start;
        gap: 16px;
    }
    .xmas-senteurs .slick-list{
        padding-left: 0 !important;
    }
    .xmas-senteurs .slick-list{
        padding-right: calc(100vw - 252px);
    }
    .xmas-senteurs .slick-dots{
    left:0;
    }
    .xmas-senteur{
        max-width: 220px;
        height: 436px;
        background-size: contain;
    }
    .xmas-senteur.vanille{
        margin-top: 0;
    }
    .xmas-senteur h3{
        font-size: 1.5rem;
    }
    .xmas-senteur a{
        font-size: .75rem;
    }
    .xmas-senteurs .slick-dots button:before, .xmas-cat .slick-dots button:before{
        content: "";
        display: inline-block;
        width: .75rem;
        height: .75rem;
        background-clip: padding-box;
        border-radius: 50%;
        -webkit-transition: opacity 75ms 0ms;
        transition: opacity 75ms 0ms;
        opacity: 1;
        background-color: #FFFFFF26;
    }
    .xmas-senteurs .slick-dots .slick-active button:before, .xmas-cat .slick-dots .slick-active button:before{
        background-color: #FFF !important;
    }

    /* TABS */
    .xmas-tabs{
        margin-top: 40px;
        border: none;
    }
    .xmas-tabs p{
        font-size: 1rem;
        margin-bottom: 20px;
    }
    .xmas-tabs a{
        font-size: .875rem;
        padding: 15px 10px;
    }
    .xmas-tabs img{
        display: none;
    }

    /* PRODUITS */
    .xmas-produits{
        padding: 20px 16px 0 !important;
    }

    /* ENCARTS */
    .xmas-encarts {
        gap: 80px;
        margin-bottom: 80px;
        flex-direction: column;
        padding: 0 16px;
    }
    .xmas-encarts article>div{
        height: 110px;
    }
    .xmas-encarts article>div a{
        font-size: .75rem;
    }

    /* CATEGORIES */
    .xmas-cat{
        flex-direction: column;
        padding: 20px 0 20px 16px;
    }
    .xmas-cat .slick-list{
        padding-right: calc(100vw - 304px);
    }
    .xmas-cat h2{
        margin-right: 0;
        text-align: center;
        padding: 0 16px !important;
    }
    .xmas-cat ul{
        max-width: 100%;
        gap: 8px;
        flex-wrap: nowrap;
        overflow: scroll;
        scrollbar-width: none;
    }
    .xmas-cat li{
        flex-shrink: 0;
    }
    .xmas-cat ul img {
        width: 78px;
    }
    .xmas-cat li span {
        font-size: .813rem;
    }

    /* FAQ */
    .xmas-faq{
        font-size: 1rem;
        margin-top: 40px;
        color: #fff;
        padding: 0 16px;
    }
    .accordion{
        padding: 0 16px 40px;
    }
    .accordion .item button{
        padding-right: 17px;
    }
}
