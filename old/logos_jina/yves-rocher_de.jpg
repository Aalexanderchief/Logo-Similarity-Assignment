        document.addEventListener('DOMContentLoaded', () => {    
            // SLIDER FILTERS
            const filters = document.querySelectorAll('#selection_filters .item');
            const allProductSlider = document.querySelectorAll('.selection div[data-tck-slot-reference]');
 
            // REMOVE AFTER CHRISTMAS
            allProductSlider.forEach(productSlider => {
                const productSliderChild = productSlider.querySelector('.row .column');
                const whiteElement = productSliderChild.querySelector('.grproductboxcontainer.background-color_white');
                whiteElement.classList.remove('background-color_white');
                if(productSliderChild.querySelector('.product-box-container_slides .slick-next')) {
                    const buttonBg = productSliderChild.querySelector('.product-box-container_slides .slick-next')
                    buttonBg.style.setProperty('background', 'none', 'important');
                }
            });
    
            function unselectAndShowProductSlider() {
                filters.forEach(filter => {
                    if (filter.classList.contains('item-selected')) {
                        filter.classList.remove('item-selected');
                    }
                });
    
                this.classList.add('item-selected');
    
                allProductSlider.forEach(productSlider => {
                    const productSliderChild = productSlider.querySelector('.row .column');
    
                    if (!productSliderChild.classList.contains('none') && (productSlider.getAttribute('data-tck-slot-reference') !== 'column4component-deatch_4cols_dbt-home_stamps')) {
                        productSliderChild.classList.add('none');
                    }
    
                    if (productSlider.getAttribute('data-tck-slot-reference') === `column2component-deatch_2cols_dbt-home_slider_pbc-${this.classList[1]}_c4`) {
                        productSlider.querySelector('.column').classList.remove('none');
                        productSlider.querySelector('div[data-js="slider"]').slick.setPosition();
                    }
                });
            }
    
            filters.forEach(filter => filter.addEventListener('click', unselectAndShowProductSlider));
        });
