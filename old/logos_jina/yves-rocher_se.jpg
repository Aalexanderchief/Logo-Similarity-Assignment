        // Si l’élément n’est pas encore présent, on utilise un MutationObserver
        if (!document.querySelector(".promo-banner-pdp")) {
            const observer = new MutationObserver((mutations, observerInstance) => {
                if (document.querySelector(".promo-banner-pdp")) {
