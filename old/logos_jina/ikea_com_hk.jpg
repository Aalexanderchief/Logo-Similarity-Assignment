            comparatorChecked: function(){
                if (this.itemComparatorList != null) {
                    let self = this
                    return this.itemComparatorList.findIndex(c => c == self.item.id) != -1
                }
                return false
            },
            comparatorCheckboxId: function(){
                return 'check_' + this.item.id
