<script type='text/javascript'>
    var cartResumeInfo = null;

    ResumeCart = new Vue({
        el: '#shoppingCartResumeComponent',
        name: 'shopping-cart-resume-component',
        data: function () {
            return {
                isMobile: window.matchMedia("only screen and (max-width: 991px)").matches,
                cartIconId: '',
                classesToApply: '',
                totalCartQuantity: 0,
                carts: [],
                cartHasBothTypes: false
            }
        },
        mounted: function() {
            this.cartIconId = this.isMobile ? 'iconShoppingMobile' : 'iconShoppingDesktop';
            this.totalCartQuantity = (cartResumeInfo == null) ? this.totalCartQuantity : cartResumeInfo.totalCartQuantity;
            this.carts = (cartResumeInfo == null) ? this.carts : cartResumeInfo.carts;
            this.cartHasBothTypes = (this.carts.food != null && this.carts.furniture != null) && (this.carts.food.totalQuantity > 0 && this.carts.food.totalPrice > 0) && (this.carts.furniture.totalQuantity > 0 && this.carts.furniture.totalPrice > 0);
            // Init move shopping cart to correct container
            $(function() {
                moveShoppingCart();
            });

            // Resize move shopping cart to correct container
            $(window).resize(function(){
                moveShoppingCart();
            });
        },
    });

    function moveShoppingCart() {
        if(window.matchMedia("only screen and (max-width: 991px)").matches){
            if ($("#shopping-cart-mobile-container > #shoppingCartResumeComponent").length < 1)
                $("#shoppingCartResumeComponent").appendTo('#shopping-cart-mobile-container');
        } else {
            if ($("#shopping-cart-desktop-container > #shoppingCartResumeComponent").length < 1)
                $("#shoppingCartResumeComponent").appendTo('#shopping-cart-desktop-container');
        }
    }

    $('html').on('click', function(e) {
        var cartIconId = document.querySelector('[id^="iconShopping"]').id;
        var countItemsId = document.querySelector('[id^="iconCart"]').id;
        var resumeIsVisible = !$("div#shoppingCartTypes").hasClass('d-none');
        var clicked = e.target.id == '' ? 'outside' : e.target.id;

        if ((cartIconId.includes(clicked) || countItemsId.includes(clicked)) && !resumeIsVisible) {
            // $("div.overlay").addClass('active');
            $("div#shoppingCartTypes").removeClass('d-none');
        } else {
            // $("div.overlay").removeClass('active');
            $("div#shoppingCartTypes").addClass('d-none');
        }
    });
</script>
