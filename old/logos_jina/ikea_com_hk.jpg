            },
            getS3DefaultImage: function (default_image) {
                return default_image.replace('_S4', '_S3');
            }
        },
        computed: {
            item_basic: function(){
                if(this.item.itemBasic != undefined){
                    return this.item.itemBasic;
                }else{
                    return this.item;
                }
            },
            has_low_stock: function(){
                return this.item_basic.stock <= 3 && this.item_basic.stock > 0;
            },
            has_no_stock: function(){
                return this.item_basic.stock == 0;
