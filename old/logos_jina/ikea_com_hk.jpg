                this.$refs.item_image.src = this.item_basic.default_image;
            }
        },
        methods: {
            price_format: function(price){
                return fullFormatPrice(price);
            },
            weight_format: function(weight){
                if(parseFloat(this.item.gross_weight) > 0 && parseFloat(this.item.gross_weight) < 0.05) {
                    return parseFloat(weight).toFixed(2);
                } else {
                    return formatWeight(weight);
                }
            },
            partnumber_format: function(partnumber){
                return Main.partnumberIKEAFormat(partnumber);
            },
            quantityLimitText: function(qty){
                var text = "每人限買[QUANTITY]件";
                return text.replace("[QUANTITY]", qty);
