            max_length: {required: false, type: Number},
        },
        methods: {
            onBlur: function(val) {
                this.isValid()
                this.inputEmit()
            },
            isValid: function() {
                this.input_text = this.input_text.replace(/^\s+/, '')
                let sizeMax = this.input_text.match(/[\u4e00-\u9eff]/) ? 12 : 25;
                this.input_error_message = (this.input_text.length > sizeMax) ? Messages.getText('MaxLimit') : Messages.getText('IsRequired');
                if (typeof this.input_text === 'string' && this.input_text.length > 0) {
                    this.input_error = false
                    return true
                } else {
                    this.input_error = true
                    return false
                }
                this.inputEmit()
            },
            inputEmit: function() {
                this.$emit('input', this.input_text)
            }
        },
        watch: {
            input_text: function(val) {
                this.isValid()
            },
            has_error: function(val) {
                this.input_error = val
            }
        }
    });
