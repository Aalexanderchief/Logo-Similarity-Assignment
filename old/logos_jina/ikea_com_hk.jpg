                    label = option[this.name_prop];
                    if(option.default != undefined && JSON.parse(option.default)){
                        label += " "+this.default_label;
                    }
                }else{
                    label = option['name'];
                }
                
                
                return label;
            }
        },
        computed: {
            input_value: {
                get:function(){
                    return this.value;
                },
