<script type='text/javascript'>
    Vue.component('select-component', {
        template: '#selectComponent',
        data: function(){
            return {
                "input_error" : this.has_error
            };
        },
        mounted: function() {
            this.updateOptions();
            this.initListeners();
        },
        props: ["value", "placeholder", "name", "has_error", "error_message", "top_label", "is_required", "container_class", "options", "onFocus", "onBlur", 'showStrongLabel'],
        methods: {
            valueChanged: function(val){
                this.$emit('change', val);
            },
            inputFocus: function(){
                this.$emit('inputfocus', this.value);
            },
            updateOptions:function(){
                var self = this;
                $.each(this.options, function(k,v){
                    if(v.value === self.input_value){
                        v.selected = true;
                        return false;
                    }
                });

                $(this.$refs['select']).empty();
                $(this.$refs['select']).select2({
                    "data": this.options,
                    "width": '100%',
                    "dropdownPosition": 'below',
                    "dropdownParent": $(this.$refs['dropdown-container'])
                });
                $(this.$el).find('b[role="presentation"]').hide();
                $(this.$el).find('.select2-selection__arrow').append('<i class="fas fa-search"></i><i class="fas icon fundament-icon-chevron-down-small mr-2"></i>');
            },
            initListeners: function(type){
                var self = this;
                $(this.$refs['select']).on('select2:select', function (e) {
                    var data = e.params.data;
                    self.input_value = data.id;
                });
                $(this.$refs['select']).on('select2:open', this.onFocus);
                $(this.$refs['select']).on('select2:close', this.onBlur);
            }
        },
        watch: {
            has_error: function(val){
                this.input_error = val;
            },
            options: function(){
                this.updateOptions();
            }
        },
        computed: {
            input_value: {
                get:function(){
                    return Number.isInteger(this.value) ? this.value.toString() : this.value;
                },
                set:function(val){
                    if(val){
                        this.$emit('input', val);
                        this.valueChanged(val);
                    }
                }
            }
        }
    });

</script>

