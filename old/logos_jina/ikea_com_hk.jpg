<script type='text/javascript'>
    Vue.component('carousel-add-to-cart-btn-component', {
        template: '#carousel-add-to-cart-btn-component-template',
        mounted: function () {
        },
        props: {
            item: Object,
            carousel_name: String,
            update_parent_shopping_cart: Function,
            force_to_show_add_on_sales: Function,
            refreshCarousel: Function,
        },
        data() {
            return {
                isWebView: '' == '1' ? true : false,
            }
        },
        methods: {
            updateCart: function(result) {
                if(typeof Cart !== 'undefined'){
                    Cart.vue.updateDataFromCarousel(result);
                }
                if (result.success) {
                    $('#positive-notification .inline-message-toast__title').text(result.items_added[0].itemBasic.name);
                    $('#positive-notification .inline-message-toast__action').remove();
                    if (!this.isWebView) {
                        this.makeGAProductClick()
                    }
                    BasicCart.showNotification('positive');
                } else {
                    BasicCart.showNotification('error');
                }
            },
            addItem: function() {
                var self = this;
                BasicCart.addItem(this.item.id, 1, function(result) {
                    //GA4 - add_to_cart
                    try {
                        let position =  ga_ecommerce_events.getIndex(self.$el);
                        let productGA4 = ga_ecommerce_events.getProductImpression(self.item.gtm, position);
                        const itemListName = ga_ecommerce_events.getItemListName(self.$el);

                        productGA4.item_list_id = itemListName;
                        productGA4.item_list_name = itemListName;
                        ga_ecommerce_events.add_to_cart({
                            'ecommerce': {
                                'value': productGA4.price,
                                'currency': 'HKD',
                                'items': [productGA4]
                            },
                            
                        });
                    } catch(ex) {
                        console.error('GA4 Error - add_to_cart');
                    }

                    // add filter to update addOnSales items from back to mount addOnSales Carousel
                    if (self.carousel_name == 'productsRecentlyViewed' && result.success) {
                        self.updateCartAddOnSales();
                    }
                    if (result.success) {
                        self.sendAddToCartEvent()
                        if (typeof NativeApp !== 'undefined') {
                            let flutterPayload = result.product_firebase ?? {tracking:{ga_datalayer:{}}}
                            if(Cart.addonsalesFirebase && Cart.addonsalesFirebase.tracking && Cart.addonsalesFirebase.tracking.ga_datalayer){
                                flutterPayload.tracking.ga_datalayer.product_list_id = Cart.addonsalesFirebase.tracking.ga_datalayer.product_list_id ?? null
                                flutterPayload.tracking.ga_datalayer.product_list_name = Cart.addonsalesFirebase.tracking.ga_datalayer.product_list_name ?? null
                                // Add insider tracking
                                flutterPayload.tracking.insider_datalayer = AppInsiderEvents.getItemAddedToCart(result.items_added);
                            }
                            NativeApp.sendAddProductToCartEvent(flutterPayload);
                        }
                    }
                    self.updateCart(result);
                });
            },
            updateCartAddOnSales() {
                var self = this
                fetch('/shoppingcart/getAddOnSales', {
                    method: 'GET',
                    cache: 'no-cache',
                    credentials: 'same-origin',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                })
                .then(result =>{
                    return result.json()
                })
                .then(result => {
                    Cart.vue.addon_sales_items = result.addon_sales_items;
                    Cart.addon_sales_items = result.addon_sales_items;
                    if (Cart.addon_sales_items.length > 0) {
                        self.force_to_show_add_on_sales(true);
                    }
                })
                .catch(error => {
                    // console.error('Error:', error)
                })
            },
            sendAddToCartEvent() {
                try{
                    var position = ga_ecommerce_events.getProductPosition(this.currentTarget);
                    var product = [ga_ecommerce_events.getProductImpression(this.item.gtm,position)]
                    ga_ecommerce_events.add_to_cart({
                        ecommerce: {
                            value: parseFloat(product[0].price),
                            currency: 'HKD'
                        },
                        items: product
                    })
                } catch (error) {}
        },
            makeGAProductClick() {
                                    try {
                        let quantity = '1';
                        let products = [GA_datalayer.getProductAddToCart(this.item.gtm, quantity)];
                        GA_datalayer.measureAddToCart(products);
                    } catch (error) {}
                            }
        },
    });
</script>

