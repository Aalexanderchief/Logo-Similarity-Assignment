<script type='text/javascript'>
    Vue.component('add-to-cart-component', {
        template: '#addToCartComponent',
        props: ['item', 'quantity', 'location'],
        data: function () {
            return {
                showCounter: this.quantity > 0 ? true : false,
                newQuantity: this.quantity,
                oldQuantity: this.quantity,
                actualQuantity: '',
                crossSellingItemQuantity: 0,
                isWebView: '' == '1' ? true : false,
            }
        },
        methods: {
            updateQuantity: function() {
                setTimeout(function(){
                    $('#shoppingCartQuantity').tooltip('hide');
                }, 2000);
                let notification = '';
                this.newQuantity = parseInt(this.newQuantity);
                if(this.newQuantity > this.oldQuantity){
                    this.addItemToCart(null, this.newQuantity);
                    notification = 'positive';
                }
                else if(this.newQuantity < this.oldQuantity){
                    this.updateItemFromCart(null, this.newQuantity);
                    notification = 'caution';
                }
            },
            addItemToCart: function(event, qty=1) {
                if (this.newQuantity === 0) {
                    this.newQuantity = 1;
                }else if(qty === 1){
                    ++this.newQuantity;
                } else {
                    let quantityLimit = this.item.quantity_limit;
                    let stock = this.item.stock;
                    let maxQuantityAvailable = (quantityLimit != null && quantityLimit > 0) ? quantityLimit : stock;
                    if (this.newQuantity > maxQuantityAvailable) {
                        this.newQuantity = maxQuantityAvailable;
                    }
                }
                if(this.newQuantity > this.item.stock){
                    BasicCart.showNotification('negative');
                    this.newQuantity = this.item.stock;
                    return;
                }
                if(this.newQuantity > this.oldQuantity){
                    let gaQuantity = this.newQuantity - this.oldQuantity;
                    //GA4 - add_to_cart
                    this.sendGA4AddToCartEvent(gaQuantity);
                    //GA3 - add_to_cart
                    this.sendGA3AddToCartEvent(gaQuantity);
                }
                this.makeRequestUpdateCart('positive', 'add');
            },
            removeItemFromCart: function(event, qty=1) {
                if (this.newQuantity === 1) {
                    this.newQuantity = 0;
                }else if(qty === 1){
                    --this.newQuantity;
                }
                if(this.newQuantity < this.oldQuantity){
                    let gaQuantity = this.oldQuantity - this.newQuantity;
                    //GA4 - add_to_cart
                    this.sendGA4RemoveFromCartEvent(gaQuantity);
                    //GA3 - add_to_cart
                    this.sendGA3RemoveFromCartEvent(gaQuantity);
                    this.makeRequestUpdateCart('caution', 'remove');
                }
            },
            updateItemFromCart: function(event, qty=1) {
                if (this.newQuantity < 1) {
                    this.newQuantity = 0;
                }
                if(this.newQuantity < this.oldQuantity){
                    let gaQuantity = this.oldQuantity - this.newQuantity;
                    //GA4 - add_to_cart
                    this.sendGA4RemoveFromCartEvent(gaQuantity);
                    //GA3 - add_to_cart
                    this.sendGA3RemoveFromCartEvent(gaQuantity);
                    this.makeRequestUpdateCart('caution', 'remove');
                }
            },
            sendGA4AddToCartEvent: function(qty) {
                try {
                    let productGA4 = ga_ecommerce_events.getProductImpression(this.item.gtm, 1, qty);
                    ga_ecommerce_events.add_to_cart({
                        'ecommerce': {
                            'value': parseFloat(productGA4.price),
                            'currency': 'HKD',
                            'items': [productGA4]
                        },
                    });
                } catch(ex) {
                    console.error('GA4 Error - add_to_cart');
                }
            },
            sendGA3AddToCartEvent: function(qty) {
                try {
                    let gtm = this.item.gtm;
                    let eventAction = 'Add to Cart';
                    let product = GA_datalayer.getDatalayerProduct(gtm, qty, eventAction);
                    let items = [product];

                    GA_datalayer.measureAddToCart(product);
                } catch(ex) {
                    console.error('GA3 Error - add_to_cart');
                }
            },
            sendGA4RemoveFromCartEvent: function(qty) {
                try {
                    let productGA4 = ga_ecommerce_events.getProductImpression(this.item.gtm, 1, qty);
                    ga_ecommerce_events.remove_from_cart({
                        'ecommerce': {
                            'value': parseFloat(productGA4.price),
                            'currency': 'HKD',
                            'items': [productGA4]
                        },
                    });
                } catch(ex) {
                    console.error('GA4 Error - remove_from_cart');
                }
            },
            sendGA3RemoveFromCartEvent: function(qty) {
                try {
                    let gtm = this.item.gtm;
                    let eventAction = 'Remove from Cart'
                    let product = GA_datalayer.getDatalayerProduct(gtm, qty);
                    let items = [product];
                    let info = GA_datalayer.getRemoveFromCartInfo(items, eventAction);

                    GA_datalayer.measureRemoveFromCart(info);
                } catch(ex) {
                    console.error('GA3 Error - remove_from_cart');
                }
            },

            isValidQuantity: function(e) {
                let keyPressed = e.key;
                this.actualQuantity = this.newQuantity;
                if (keyPressed == 'Backspace' || keyPressed == 'Delete') {
                    let inputValue = e.target.value;

                    if (inputValue.length == 0) {
                        this.newQuantity = '0';
                    }

                    return;
                }

                if (/^[0-9]$/.test(keyPressed)) {

                    let inputValue = e.target.value;

                    // if customer deleted the input, the input value in the field should be 0
                    if (inputValue == 0) {
                        // replace the 0 by the value
                        this.newQuantity = keyPressed;
                    } else {
                        // concatenate the previous digits with the new one
                        this.newQuantity += keyPressed;
                    }
                }

                e.preventDefault();
            },
            deleteNonDigits: function(e) {
                if (e.key == 'Unidentified') {
                    let quantityToUpdate = this.actualQuantity.toString();
                    this.newQuantity = quantityToUpdate.replace(/[^(?!0-9)]*/, '');
                    e.preventDefault();
                }
            },
            updateQuantityWhenDeleted: function() {
                if(this.newQuantity == '')
                this.newQuantity = '0';
            },
            makeRequestUpdateCart: function(notificationType, mode) {
                this.oldQuantity = this.newQuantity;
                let self = this;
                let callback = function(result) {
                    if (result.success != false && !result.warning_message) {
                        let itemName = self.crossSellingInlineHasItems() && self.location != 'sidenav' ? self.item.name + ' ' + $('.csi_add_title').text() : self.item.name;
                        BasicCart.showNotification(notificationType, itemName, self.location);
                        if (mode === 'add') {
                            // Algolia add to cart
                            let algoliaItems = AlgoliaEvents.getItemFromLocalStorage('clickedObjectIDsAfterSearch', self.item.id);
                            if(typeof algoliaItems !== 'boolean'){
                                AlgoliaEvents.setLocalChanges('convertedObjectIDsAfterSearch', self.item.id, algoliaItems);
                            }
                        }
                    } else if (result.warning_message) {
                        BasicCart.showNotification('added-with-warning', result.warning_message);
                    } else {
                        BasicCart.showNotification('error');
                    }
                };
                let itemsToAdd = [{
                    'id': this.item.id,
                    'quantity': this.newQuantity
                }];

                if (this.crossSellingInlineHasItems() && this.location != 'sidenav') {
                    this.updateCrossSellingInlineItemQuantity(mode);
                    itemsToAdd.push({
                        'id': this.getItemFromCrossSellingInline(),
                        'quantity': this.crossSellingItemQuantity
                    });
                }
                BasicCart.addItems(itemsToAdd, 'update_item', callback);
            },
            crossSellingInlineHasItems: function() {
                let crossSellingInlineHasItems = false;
                if ((typeof CrossSelling !== "undefined") && (typeof CrossSelling.selectedItem !== "undefined") && (CrossSelling.selectedItem.id !== null)) {
                    crossSellingInlineHasItems = true;
                }
                return crossSellingInlineHasItems;
            },
            getItemFromCrossSellingInline: function() {
                return CrossSelling.selectedItem.id.toString();
            },
            updateCrossSellingInlineItemQuantity: function(mode) {
                if (mode == 'add') {
                    ++this.crossSellingItemQuantity
                } else {
                    this.crossSellingItemQuantity = this.crossSellingItemQuantity == 0 ? 0 : --this.crossSellingItemQuantity;
                }
            },
        },
    });
</script>


