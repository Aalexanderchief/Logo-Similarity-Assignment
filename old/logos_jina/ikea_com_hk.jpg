<script type='text/javascript'>
    Vue.component('input-phone-component', {
        template: '#input-phone-component',
        data: function(){
            return {
                intl_phone: null,
                input_error: this.has_error,
                unique_id: this.name+"_"+Math.floor(Math.random() * 1000),
                is_focus: false,
                current_phone: "" ,
                current_error_message: this.error_message,
                formated_phone_max_length:  14,
                phone_max_length:  8,
                countries: {
                    hk: 852,
                    mo: 853
                },
                countryDialCode: "852", // HK Default dial code
            };
        },
        props: [
            "value",
            "placeholder",
            "name",
            "has_error",
            "error_message",
            "top_label",
            "is_required",
            "type",
            "container_class",
            "remove_margin",
            "force_show_label"
        ],
        mounted() {
                const intl_phone = document.querySelector("#"+this.unique_id);
                this.intl_phone = window.intlTelInput(intl_phone, {
                    formatOnDisplay: false,
                    separateDialCode: true,
                    initialCountry: "hk",
                    onlyCountries: ['hk', 'mo'],
                    useFullscreenPopup: false,
                    countrySearch: false,
                    showSelectedDialCode: true,
                    utilsScript: "/webroot/js/vendor/intltelinput/utils.js"
                });

                if(parseInt(this.value) != NaN && validatePhone(this.value)){
                    this.intl_phone.setNumber(this.value)
                    this.countryDialCode = this.intl_phone.getSelectedCountryData().dialCode
                    this.current_phone = this.value ?? ""
                    if(this.current_phone != ""){
                        this.isPhoneValid()
                    }
                }

                let self = this
                intl_phone.addEventListener('countrychange', (e, country) => {
                    self.countryDialCode = self.intl_phone.getSelectedCountryData().dialCode
                    self.valueChanged(e)
                });
        },
        methods: {
            inputFocus: function(val) {
                this.is_focus = !this.is_focus;
                this.input_error = false;
            },
            inputBlur: function(val) {
                this.is_focus = false
                this.isPhoneValid()
            },
            valueChanged: function(e) {
                if (e) {
                    let value = e.currentTarget.value
                    value = checkPhoneLength(value)

                    if (value && value.length <= this.formated_phone_max_length) {
                        this.current_phone = value
                    }
                }
                this.emitValue();
            },
            isPhoneValid() {
                let phone = this.value ?? ""
                if(phone.includes('+')){
                    phone = phone.replace(/^\+[0-9]{1,3}/, "")
                }
                phone = phone.replace(/(-)/, "")
                phone = phone.replace(' ', "")

                if(!this.is_required && phone.length == 0){
                    return true
                }else if (phone.length != this.phone_max_length) {
                    this.current_error_message = Messages.getText('PhoneLength')
                    this.input_error = true
                    return false
                } else if (validatePhone(this.value)) {
                    this.current_error_message = Messages.getText('PhoneFormat')
                    this.input_error = false
                } else {
                    this.current_error_message = Messages.getText('PhoneFormat')
                    this.input_error = true
                }

                return true
            },
            emitValue: function() {
                let newValue = this.current_phone.replace("-","")
                if(newValue.length > 0 && this.countryDialCode != undefined ){
                    newValue = '+' + this.countryDialCode + this.current_phone.replace("-","")
                }
                this.$emit('input', newValue)
            },
            onPaste: function(evt) {
                evt.preventDefault();
                let pastedCurrentPhoneString = event.clipboardData.getData('text').substring(0, 8);
                this.current_phone = Main.convertFullwidthDigitsToRegularDigits(pastedCurrentPhoneString);
                this.emitValue();
            },
        },
        computed: {
            phone_description() {
                this.input_error = (this.countryDialCode == undefined);
                return '+' + this.countryDialCode + '-0000-0000'
            }
        },
        watch: {
            has_error: function(val) {
                this.input_error = val
            },
            current_phone(newVal) {
                this.current_phone = beautifyPhone(newVal)
            },
        }
    });
</script>
