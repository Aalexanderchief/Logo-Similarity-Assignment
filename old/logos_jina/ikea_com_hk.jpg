            item: Object,
            is_mobile: Boolean,
            update_parent_shopping_cart: Function,
            force_to_show_add_on_sales: Function,
            refreshCarousel: Function,
            ga_list: String,
            show_last_price: {
                type: Boolean,
                default: true
            }
        },
        data() {
            return {
                main_img_src: this.item.default_image,
                isHover: false,
                forceToRefreshImg: false,
                isWebView: '' == '1' ? true : false,
                updated_img: '',
            }
        },
        updated() {
            if (this.forceToRefreshImg) {
                this.main_img_src = this.item.default_image
                this.forceToRefreshImg = false
            }
        },
        methods: {
            increaseRefresh(){
                Comparator.setLastComparedTraffic(this.$parent)
                Comparator.sortTrafficInstances()
                Comparator.refreshTrafficInstances()
            },
            refreshComparatorBarVisibility() {
                this.$emit('refreshComparatorBarVisibility');
            },
            test() {
                this.isHover = !this.isHover
            },
            handleHover: function(src) {
                if (this.isWebView == false) {
                    this.updated_img  = src;
                }
            },
            refreshImg() {
                this.forceToRefreshImg = true
            },
            dateToDMA(date) {
                let d = date.getDate()
                let m = date.getMonth() + 1
                let y = date.getFullYear()
                return (d <= 9 ? '0' + d : d) + '-' + (m<=9 ? '0' + m : m) + '-' + y
            },
            makeGAProductClick() {
                if (!this.isWebView) {
                    let product = [GA_datalayer.generateImpressionProductJson(this.productInfo, this.ga_list, this.index + 1)];
                    GA_datalayer.measureProductClick(this.ga_list, product);
                    //GA4 - select_item
                    try {
                        let position = ga_ecommerce_events.getIndex(this.$el);
                        let productGA4 = ga_ecommerce_events.getProductImpression(this.productInfo.gtm, position);
                        const itemListName = ga_ecommerce_events.getItemListName(this.$el);

                        ga_ecommerce_events.select_item({
                            'item_list_id' : itemListName,
                            'item_list_name' : itemListName,
                            'ecommerce': {
                                'value': productGA4.price,
                                'currency': 'HKD',
                                'items': [productGA4]
                            },
                            
                        });
                    } catch(ex) {
                        console.error('GA4 - select_item');
                    }
                }
                //NativeApp Tracking Event
                try {
                    if(typeof NativeApp !== 'undefined' && typeof Cart !== 'undefined') {
                        let data = Cart.addonsalesFirebase;
                        //Adding the selected profuct to GA data
                        data.tracking.ga_datalayer.product = this.productInfo.appGa4;
                        //Unset 'products' key from GA data
                        delete data.tracking.ga_datalayer.products;
                        //Send event to App
                        NativeApp.sendSelectProductEvent(data);
                    }
                } catch(ex) { /*Do nothing*/ }
            },
            showYuuPrice(){
                return ((this.item.activitystartdate == null && this.item.activitystartdate == null) && !this.isFamilyPrice && !this.isBusinessPrice && !this.isNormalOffer);
            }
        },
        computed: {
            showVariations() {
                return (this.isHover)
                        && (this.item.itemColorVariation != null
                        && this.item.itemColorVariation.totalItemRelations > 0)
            },
            getImgSrc() {
                let item = this.item
                return item.default_image
            },
            activityDatePrice() {
                let start  = new Date(this.item.activitystartdate)
                let end  = new Date(this.item.activityenddate)
                return this.dateToDMA(start) + ' ~ ' +  this.dateToDMA(end)
            },
            productInfo() {
                let item = this.item
                let productInfo = {
                    name: this.item.gtm.name,
                    brand: this.item.name,
                    id: this.item.partnumber.toString(),
                    price: this.item.current_price.toString(),
                    category: this.item.full_category,
                    variant: this.item.type
                }

                if(this.item.gtm != null) {
                    delete this.item.gtm.quantity
                    delete this.item.gtm.metric1
                    delete this.item.gtm.metric2
                    delete this.item.gtm.actionFieldName

                    productInfo = {...productInfo, gtm: this.item.gtm}
                }

                //Setting GA4 for App data
                if(this.item.appGa4 != null) {
                    productInfo.appGa4 = this.item.appGa4;
                    productInfo.appGa4.index = this.index + 1;
                }

                return productInfo;
            },
            isNormalOffer: function(){
                return (this.item.discount > 0 && this.item.offer_price < this.item.original_price);
            },
            isYuuPrice: function(){
                return typeof this.item.yuu_price !== 'undefined' && this.item.yuu_price != null && this.item.yuu_price > 0;
            },
            isFamilyPrice: function() {
                let memberType = localStorage.getItem('member_type');
                return memberType !== 'Business' && this.item.family_price != null && this.item.family_price > 0;
            },
            isBusinessPrice: function() {
                let memberType = localStorage.getItem('member_type');
                return memberType === 'Business' && this.item.business_price != null && this.item.business_price > 0;
            },
            isNLP: function() {
                let memberType = localStorage.getItem('member_type');
                return memberType !== 'Business' && this.item.hbp_price != null && this.item.hbp_price > 0 && !this.showYuuPrice && !this.item.is_bti;
            },
        },
    });
</script>

