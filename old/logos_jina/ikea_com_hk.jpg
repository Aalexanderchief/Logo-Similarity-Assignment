                return get_units(units, metrics)                
            }
        },
        computed: {
            isNormalOffer: function(){
                return this.item.offer_price > 0 && !this.item.is_bti && !this.item.is_nlp;
            },
            isYuuPrice: function(){
                return this.item.yuu_price != null && this.item.yuu_price > 0 && this.show_yuu_price
            },
            isActivityOffer: function() {
                let startDate = new Date(this.item.activitystartdate);
                let endDate = new Date(this.item.activityenddate);

                return parseFloat(this.item.offer_price) > 0 && parseFloat(this.item.original_price) > parseFloat(this.item.offer_price)
