                let isCheckboxDisabled = e.target.parentElement.children[0].disabled;
                if(isCheckboxDisabled && Comparator.getHideComparatorBarValue()){
                    $(".listComparator").removeClass('d-none');
                    $(".listComparator").addClass('d-block');
