<script type='text/javascript'>
    Vue.component('carousel-component', {
        template: '#carouselComponent',
        name: 'carousel-component',
        props: {
            items: Array,
            title: String,
            type: String,
            name: String,
            id_name: String,
            full_traffic_id: String,
            checkout_step: [Number, String],
            total_items: Number,
            items_to_display: Object,
            items_to_slide: Object,
            update_parent_shopping_cart: Function,
            force_to_show_add_on_sales: Function,
            ga_list: String,
            progress_bar: Boolean,
            loop: Boolean,
            is_first_traffic: Boolean
        },
        data() {
            return {
                is_mobile: false,
                margin_top: this.type == 'home_page' ? 'mt-0 mb-0' : '',
                lastVisibleItemsForGa4: [],
                currentVisibleItems: [],
                is_mounted: false,
                isVisible: false,
                isWebView: '' == '1',
                comparatorActive: false,
                refresh: 0,
                observerRootMargin: '0px',
                // ============================ KEEN SLIDER ============================
                slider: null,
                sliderId: 'keen-slider-' + this.name,
                imagesUrlItems: this.getImagesItems(),
                sliderPerClick: 1,
                config: {},
                loaded: [],
                // ============================ NAVIGATION ============================
                navigationWrapperClass: 'navigation-wrapper-'+ this.name,
                showProgressbar: false,
                elementIndexSelected: 0,
                show_last_price: true,
                // ============================ NATIVEAPP GA EVENT ============================
                viewProductListSended: []
            }
        },
        created: function() {
            window.addEventListener('scroll', this.handleScroll);
        },
        mounted() {
            this.is_mobile = isMobileView()
            this.refreshCarousel()
            this.makeWindowResizeEvent()

            // Add the traffic and make it visible (required for grouped traffics)
            if (typeof Comparator != 'undefined') {
                Comparator.addNewTrafficInstance(this)
                if (this.full_traffic_id == Comparator.getLastComparedTraffic()) {
                    Comparator.triggerClickOnGroupedTraffic(this.full_traffic_id)
                }
            }
        },
        methods: {
            handleScroll: function() {
                this.refreshObserverRootMargin();
            },
            increaseRefresh() {
                this.refresh++;
                this.updateComparatorActive()
            },
            updateComparatorActive(){
                this.comparatorActive = (typeof Comparator != 'undefined')
                    ? this.full_traffic_id == Comparator.getLastComparedTraffic()   // Active traffic
                    : false
            },
            getKeysWithUnderscore(text) {
                // Return from 'keen_slider_54890_6599_713' to '54890_6599_713'
                const digits = text.match(/\d+/g);

                if (!digits) {
                    return "";
                }

                if (digits.length === 1) {
                    return digits[0];
                }

                return digits.join('_');
            },
            initCarousel() {
                let vm = this
                    try {
                        if (vm.slider) {
                            vm.slider.destroy()
                            vm.slider = null;
                        }
                        vm.config = {
                            loop: vm.loop,
                            drag: vm.total_items > 4,
                            slides: {
                                perView: vm.items_to_display.desktop,
                            },
                            spacing: 5,
                            breakpoints: {
                                '(max-width: 1024px)': {
                                    loop: vm.loop,
                                    drag: vm.total_items > 3,
                                    slides: { perView: vm.items_to_display.tablet },
                                    spacing: 3,
                                },
                                '(max-width: 600px)': {
                                    loop: vm.loop,
                                    drag: vm.total_items > 2,
                                    slides: { perView: vm.items_to_display.mobile },
                                    spacing: 0,
                                },
                            },
                            is_detail_slider: true
                        }

                        vm.config.updated = (e) => {
                            vm.is_mounted = true
                            const slideIdx = e.track.details.rel;
                            let maxItems = slideIdx + vm.getMaxItems()
                            for (let idx = slideIdx; idx < maxItems; idx++) {
                                if (vm.imagesUrlItems[idx]) {
                                    vm.loaded[idx] = true;
                                    e.slides[idx].querySelector("img").setAttribute("data-src", vm.imagesUrlItems[idx]);
                                }
                            }
                        };
                        this.$nextTick(() => {
                            if(document.querySelector('#' + vm.sliderId)){
                                vm.slider = new KeenSlider('#' + vm.sliderId, vm.config, vm.getPlugins())
                            }else{
                                throw `${'#' + vm.sliderId} element not found in DOM`;
                            }

                            if (!vm.isWebView) {
                                vm.slider.on('animationEnded', () => {
                                    vm.makeGATrafficImpressionEvent()
                                })
                            }

                            if(typeof NativeApp !== 'undefined') {
                                vm.slider.on('animationEnded', () => {
                                    vm.sendNativeAppEvent();
                                })
                            }

                            vm.slider.on('slideChanged', (e) => {
                                const slideIdx = e.track.details.rel;
                                let maxItems = slideIdx + vm.getMaxItems()
                                for (let idx = slideIdx; idx < maxItems; idx++) {
                                    if (typeof e.slides[idx] !== 'undefined' && !vm.loaded[idx] && vm.imagesUrlItems[idx]) {
                                        vm.loaded[idx] = true;
                                        e.slides[idx].querySelector("img").setAttribute("src", vm.imagesUrlItems[idx]);
                                    }
                                }
                            })


                            vm.slider.on("detailsChanged", (e) => {
                                if (!vm.slider.disabledScroll && vm.haveToShowProgressbar()) {
                                    vm.getCurrentIdxSelected()
                                }
                            })

                            if (vm.type == 'home_page' || vm.type == 'custom_page') {

                                let sliderKey = vm.getKeysWithUnderscore(vm.name);

                                KeenSliderConfig.updateCarouselFunctions.push(vm.refreshCarousel)
                                KeenSliderConfig.updateCarouselFunctionKeys.push(sliderKey)

                            }

                            vm.showProgressbar = vm.haveToShowProgressbar()
                            vm.elementIndexSelected = 0;
                        });
                    } catch (e) {
                    }
            },
            makeWindowResizeEvent() {
                let vm = this
                try {
                    $(window).resize(function() {
                        vm.is_mobile = isMobileView()
                        vm.showProgressbar = vm.haveToShowProgressbar()
                        vm.initCarousel()
                    });
                } catch (e) {
                    //Do nothing
                }
            },
            refreshCarousel() {
                try {
                    if (this.slider) {
                        this.slider.destroy()
                    }
                    this.initCarousel()
                } catch (e) {
                    //Do nothing
                }
            },
            refreshComparatorBarVisibility() {
                if (typeof this.$refs["comparator-bar"] != 'undefined') {
                    this.$refs["comparator-bar"].refreshVisibility();
                }
            },


            // PROGRESS BAR
            getCurrentIdxSelected() {
                this.elementIndexSelected = this.slider.track.details.progress * this.items.length * 100;
            },
            moveToIdx(idx) {
                this.slider.moveToIdx(idx);
            },
            haveToShowProgressbar() {
                                return (window.innerWidth < 800 && this.total_items > 3) || (window.innerWidth >= 800 && this.progress_bar);
                return this.needArrows();

                                let need = false
                let vm = this
                if (Object.keys(this.config).length) {
                    switch (true) {
                        case window.innerWidth > 1024:
                            this.slidesPerClick = vm.items_to_slide?.desktop ?? vm.items_to_display.desktop
                            need = this.items.length > this.slidesPerClick;
                            break;
                        case window.innerWidth > 600 && this.config.breakpoints['(max-width: 1024px)'].loop:
                            this.slidesPerClick = vm.items_to_slide?.tablet ?? vm.items_to_display.tablet
                            need = this.items.length > this.slidesPerClick;
                            break;
                        case window.innerWidth <= 600 && this.config.breakpoints['(max-width: 600px)'].loop:
                            this.slidesPerClick = vm.items_to_slide?.mobile ?? vm.items_to_display.mobile
                                                        need = this.items.length > this.slidesPerClick;
                            break;
                        default:
                        break;
                    }
                }
                return (this.progress_bar) ? need : false;
            },
            getPlugins() {
                let plugins = []
                if (this.needArrows()) {
                    KeenSliderConfig.slidesPerClick = this.slidesPerClick
                    plugins.push(KeenSliderConfig.arrows)
                }
                return plugins
            },
            needArrows() {
                let need = false
                let vm = this
                if (Object.keys(this.config).length) {
                    switch (true) {
                        case window.innerWidth > 1024:
                            this.slidesPerClick = vm.items_to_slide?.desktop ?? vm.items_to_display.desktop
                            need = this.items.length > this.slidesPerClick;
                            break;
                        case window.innerWidth > 600:
                            this.slidesPerClick = vm.items_to_slide?.tablet ?? vm.items_to_display.tablet
                            need = this.items.length > this.slidesPerClick;
                            break;
                        case window.innerWidth <= 600 && this.config.breakpoints['(max-width: 600px)'].loop:
                            this.slidesPerClick = vm.items_to_slide?.mobile ?? vm.items_to_display.mobile
                            need = this.items.length > this.slidesPerClick;
                            break;
                        default:
                        break;
                    }
                }
                return need;
            },
            refreshImgs() {
                if(this.$refs.card != undefined){
                    this.$refs.card.forEach(card => {
                        card.refreshImg()
                    });
                }
            },
            visibilityChanged(isVisible, entry) {
                this.isVisible = isVisible
                this.lastVisibleItemsForGa4 = [];
            },
            sendEventTrafficVisibility(isVisible, entry){
                //Native app event
                if(typeof NativeApp !== 'undefined' && isVisible && this.is_mounted) {
                    this.sendNativeAppEvent();
                }
            },
            pushImpressedItem(item_id) {
                this.impressed_items.push(item_id)
            },
            getCurrentVisibleItems() {
                let currentVisibleItems = []
                // more than 60% visible
                let currentVisibleStack = this.slider.track.details.slides.filter(s => s.portion > 0.6)
                // if first load
                if (currentVisibleStack.length == 0) {
                    currentVisibleStack = this.items.slice(0, 5)
                }
                let carouselLength = this.slider.track.details.slides.length
                currentVisibleStack.forEach((slide, key) => {
                    let completedCycles = Math.trunc(slide.abs/carouselLength)
                    //Fixing abs to always get a real item index
                    let fixedAbs = slide.abs - carouselLength * completedCycles             //Fixing index
                    fixedAbs = fixedAbs >= 0 ? fixedAbs : carouselLength + fixedAbs    //Fixing sign
                    if (this.$refs.card[fixedAbs]) {
                        currentVisibleItems.push({
                            index: this.$refs.card[fixedAbs].index,
                            product: this.$refs.card[fixedAbs].productInfo
                        })
                    }
                });

                return currentVisibleItems
            },
            makeGATrafficImpressionEvent() {
                try {
                    if (this.ga_list != null) {
                        let currentVisibleItems = this.getCurrentVisibleItems();
                        let currentVisibleItemsIds = [];

                        let impressions = [];
                        let productsGa4 = [];

                        // check if the item was impressed.
                        currentVisibleItems.forEach(item => {
                            /*
                            *   item.index => index in carousel
                            *   item.product => productInfo
                            */
                            impressions.push(GA_datalayer.generateImpressionProductJson(item.product.gtm, this.ga_list, item.index + 1))
                            try {
                                currentVisibleItemsIds.push(item.product.id);
                                if (!this.lastVisibleItemsForGa4.includes(item.product.id)) {
                                    let productGa4 = ga_ecommerce_events.getProductImpression(item.product.gtm, item.index + 1);
                                    productsGa4.push(productGa4);
                                }
                            } catch(ex) {
                                //Do nothing
                            }
                        });

                        this.lastVisibleItemsForGa4 = currentVisibleItemsIds;

                        if (productsGa4.length > 0) {

                                                            GA_datalayer.measureTrafficImpression(impressions);
                            
                            //GA4 - view_item_list

                            try {
                                const itemListName = this.title != '' ? this.title : ga_ecommerce_events.getItemListName(this.$el);

                                ga_ecommerce_events.view_item_list({
                                    'item_list_id': itemListName,
                                    'item_list_name': itemListName,
                                    'ecommerce': {
                                        'value': ga_ecommerce_events.getValue(productsGa4),
                                        'currency': 'HKD',
                                        'items': productsGa4
                                    },
                                });
                            } catch(ex) {
                                console.error('GA4 Error - view_item_list');
                            }
                        }
                    }
                } catch(ex) {
                    //DO NOTHING
                }
            },
            //Tracking Native App event (view_product_list)
            sendNativeAppEvent: function() {
                try {
                    //Only for AddOnSales on ShoppingCart (view_product_list event)
                    if(typeof NativeApp !== 'undefined' && typeof Cart !== 'undefined') {
                        let visibleItems = this.getCurrentVisibleItems()
                            .filter(visibleItem => {
                                return this.viewProductListSended.findIndex(productSended => {
                                    return productSended.product.id === visibleItem.product.id;
                                }) === -1
                            });
                        let productsToSendToNativeApp = [];

                        if(visibleItems.length == 0){ return; }

                        this.viewProductListSended = [...this.viewProductListSended, ...visibleItems]

                        visibleItems.forEach((visibleItem) => {
                            productsToSendToNativeApp.push(visibleItem.product.appGa4);
                        });

                        let dataForNativeAppEvent = Cart.addonsalesFirebase;
                        dataForNativeAppEvent.tracking.ga_datalayer.products = productsToSendToNativeApp;

                        NativeApp.sendViewProductListEvent(dataForNativeAppEvent);
                    }
                } catch(ex) {
                    /* Do nothing */
                }
            },
            getImagesItems() {
                let imagesItems = [];
                this.items.forEach((item, idx) => {
                    imagesItems[idx] = item.default_image;
                });

                return imagesItems;
            },
            getMaxItems() {
                let selfWidth = window.innerWidth;
                let maxItems = 5;
                switch (selfWidth !== null) {
                    case selfWidth >= 1024:

                        break;
                    case selfWidth >= 600:
                        maxItems = 4;

                        break;
                    case selfWidth < 600:
                        maxItems = 3;

                        break;
                }
                return maxItems;
            },
            getReactItemsByWidth() {
                if (window.innerWidth <= 600) {
                    return this.items_to_display.mobile;
                } else if (window.innerWidth <= 1024) {
                    return this.items_to_display.tablet;
                } else {
                    return this.items_to_display.desktop;
                }
            },
            scrollInputHandler(e) {
                let value;
                if (this.slider.options.loop) {
                    const images = this.getReactItemsByWidth();
                    value = Number($(e.currentTarget).val()) / 100 / images;
                } else {
                    value = Number($(e.currentTarget).val()) / 100 / ((this.total_items - 1) / this.slider.track.details.length);
                }
                this.slider.track.to(value);
            },
            dragStartHandler() {
                this.slider.disabledScroll = true;
            },
            dragStopHandler() {
                this.slider.disabledScroll = false;
            },
            refreshObserverRootMargin: function() {
                this.observerRootMargin = '-' + getHeightOfStickyTopNavbar() + 'px 0px 0px 0px';
            }
        },
        watch: {
            items(new_items) {
                this.refreshImgs()
                this.refreshCarousel()
                this.imagesUrlItems = this.getImagesItems();
