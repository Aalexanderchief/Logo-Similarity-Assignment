<script>
    function price_format (price) {
        return fullFormatPrice(price);
    }

    function get_units(units, metrics) {
        return "/ " + units + " " + metrics;
    }

    function fullFormatPrice(priceStr) {
        let price = formatPrice(priceStr);
        let integer = price.split(Main.currency.dec_separator)[0]
        let decimal = price.split(Main.currency.dec_separator)[1]
        let result = document.createElement("span");
        let currencyElement = document.createElement("span")
        currencyElement.className = "currency"
        currencyElement.append(Main.currency.currency)
        let decimalElement = document.createElement("span")
        decimalElement.className = "priceDecimalFormat"
        decimalElement.append(Main.currency.dec_separator)
        decimalElement.append(decimal)


        switch(Main.currency.position) {
            case 1:
                                result.append(currencyElement)
                result.append(integer)
                if (decimal)
                    result.append(decimalElement)
                break;
            default:
                                result.append(integer)
                if (decimal)
                    result.append(decimalElement)
                result.append(currencyElement)
                break;
        }
        return result.innerHTML;
    }

function formatPrice(priceStr)
{
    priceStr = parseFloat(priceStr).toFixed(Main.currency.dec_number).toString();
    let x   = priceStr.split('.');
    let x1  = x[0];
    let x2  = "";
    let rgx = /(\d+)(\d{3})/;
    while (rgx.test(x1)) {
        x1 = x1.replace(rgx, '$1' + Main.currency.thousand_separator + '$2');
    }
    if (Main.currency.dec_number > 0 && x.length > 1 && x[1] != "0"){
        x2  = Main.currency.dec_separator+x[1].substring(0,Main.currency.dec_number);
    }

    return x1 + x2;
}
</script>
