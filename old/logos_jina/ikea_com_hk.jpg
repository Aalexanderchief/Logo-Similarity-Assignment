    // Check for IntersectionObserver support
    if ('IntersectionObserver' in window) {
        document.addEventListener("DOMContentLoaded", function() {

            function handleIntersection(entries) {
                entries.map((entry) => {
                    if (entry.isIntersecting) {
                    // Item has crossed our observation
                    entry.target.style.backgroundImage = "url('"+entry.target.dataset.bgimage+"')";
                    // Job done for this item - no need to watch it!
                    observer.unobserve(entry.target);
                    }
                });
            }

            const dealBlocks = document.querySelectorAll('.replaceBgImage');
            const observer = new IntersectionObserver(handleIntersection, { rootMargin: "100px" });
            dealBlocks.forEach(dealBlock => observer.observe(dealBlock));
        });
    } else {
        // No interaction support? Load all background images automatically
        const dealBlocks = document.querySelectorAll('.replaceBgImage');
        dealBlocks.forEach(dealBlock => {
            dealBlock.style.backgroundImage = "url('"+dealBlock.dataset.bgimage+"')";
