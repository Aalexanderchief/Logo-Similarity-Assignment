<script type='text/javascript'>
    Vue.component('personal-data-component', {
        template: '#personal-data-component',
        data: function(){
            return {
                data: this.client,
                prevClientData: {
                    name: null,
                    last_name: null,
                    phone: null
                },
                fields: {
                    name: {max_length: 25},
                    last_name: {max_length: 25},
                },
                has_error_name: false,
                has_error_last_name: false,
                has_error_phone: false,
                editing: false,
                groupClass:'personal-data-group',
            };
        },
        props: {
            client: {required: true, type: Object},
        },
        mounted: function() {
            this.prevClientData.name = this.client.name;
            this.prevClientData.last_name = this.client.last_name;
            this.prevClientData.phone = this.client.phone;
        },
        methods: {
            edit: function(val) {
                this.editing = true;
                if(typeof ga_ecommerce_events != 'undefined'){
                    try {ga_ecommerce_events.update_account_details()} catch (error) {}
                }
            },
            cancel: function(val) {
                this.editing = false
                this.has_error_name = false;
                this.has_error_last_name = false;
                this.has_error_phone = false;
                this.client.name = this.prevClientData.name;
                this.client.last_name = this.prevClientData.last_name;
                this.client.phone = this.prevClientData.phone;
            },
            dynamicLimit: function(field) {
                this.fields[field].max_length = this.client[field].match(/[\u4e00-\u9eff]/) ? 12 : 25;
            },
            isValid: function() {
                try {
                    this.client.name =  this.client.name.trim()
                    this.client.last_name =  this.client.last_name.trim()
                    this.has_error_phone = !validatePhone(this.client.phone)
                    this.has_error_name = typeof this.client.name !== 'string' || this.client.name.length === 0 || this.client.name.length > this.fields.name.max_length
                    this.has_error_last_name = typeof this.client.last_name !== 'string' || this.client.last_name.length === 0 || this.client.last_name.length > this.fields.last_name.max_length
                    if (this.has_error_phone) {
                        var alertText = Messages.getText('phone_format');
                        toastr.error(alertText);
                    }
                    if (this.has_error_name) {
                        let alertText = typeof this.field !== 'undefined' ? this.field.name.error_message : null;
                        if(alertText){
                            toastr.error(alertText) ;
                        }
                    }
                    if (this.has_error_phone) {
                        let alertText = typeof this.field !== 'undefined' ? this.field.last_name.error_message : null;
                        if(alertText){
                            toastr.error(alertText) ;
                        }
                    }

                    return !this.has_error_phone && !this.has_error_name && !this.has_error_last_name
                } catch (error) {
                    console.error(error)
                    return false
                }
            },
            save: function(event) {
                try {
                    if (this.isValid()) {
                        let genderSel = null
                        let self = this

                        let group = $('.' + this.groupClass);

                        if (group.find("input[name='createAccount_gender-option-pi'][value='male']").is(":checked")) {
                            genderSel = $("input[name='createAccount_gender-option-pi'][value='male']").val();
                        } else if (group.find("input[name='createAccount_gender-option-pi'][value='female']").is(":checked")) {
                            genderSel = $("input[name='createAccount_gender-option-pi'][value='female']").val();
                        }

                        let formData = new FormData()

                        formData.append('csrf_token', $("#account_form").val());
                        formData.append('name', this.client.name);
                        formData.append('last_name', this.client.last_name);
                        formData.append('birthday', genderSel);
                        formData.append('genderSel', genderSel);
                        formData.append('phone', this.client.phone);

                        $(group).busyLoad("show", {
                            animation: "fade"
                        });

                        $.ajax({
                            async: true,
                            type: "POST",
                            data: formData,
                            dataType: "json",
                            contentType: false,
                            processData: false,
                            url: validateURL.URL() + '/ecoclient/account_change_data',
                            success: function(result) {
                                $(group).busyLoad("hide", {
                                    animation: "fade"
                                });
                                if (result.success == false) {
                                    if (result.fields != undefined) {
                                        for (var i = 0; i < result.fields.length; i++) {
                                            ValidateClient.tagError(result.fields[i].field, result.fields[i].text);
                                        }
                                    }

                                    $("#validateOtherPhoneAccount").click(Account.validateChangePhone);
                                } else {
                                    client_account_edit(null, event.target, group);

                                    // Update previous client data for cancel function
                                    self.prevClientData.name = result.client.name;
                                    self.prevClientData.last_name = result.client.lastname;
                                    self.prevClientData.phone = result.client.phone;

                                    var nameHeader = ($('ul.userMenu li a#nameUser').html()).split(" ");
                                    $('ul.userMenu li a#nameUser').html(nameHeader[0] + " " + self.client.name);
                                    
                                }
                                self.editing = false
                            },
                            timeout: 10000
                        });
                    }
                } catch (e) {}
            },
        },
    });
</script>
